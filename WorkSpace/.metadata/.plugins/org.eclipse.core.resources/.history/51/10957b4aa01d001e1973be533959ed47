<mapper namespace="cu">

	<select id="one" resultType="customer.CustomerVO">
		select * from customer where id = #{id} and name like '%' || #{name} || '%'
	</select>
	
	<select id="list" resultType="customer.CustomerVO">
		select * from customer
	</select>

